{
  "name": "cooper-csesg",
  "version": "0.0.2",
  "description": "WIP boilerplate for Cooper Union Computer Science and Entrepreneurship for Social Good course",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^1.1.1",
    "axios": "^0.18.0",
    "express-mailer": "^0.3.1",
    "firebase": "^5.4.1",
    "google-fonts": "^1.0.0",
    "lodash": "^4.17.10",
    "material-ui": "^0.20.2",
    "materialize-css": "^1.0.0-rc.2",
    "node-sass-chokidar": "^1.3.3",
    "nodemailer": "^4.6.8",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-firebase-file-uploader": "^2.4.2",
    "react-html-parser": "^2.0.2",
    "react-http-request": "^1.0.4",
    "react-iframe": "^1.2.0",
    "react-images-upload": "^1.2.2",
    "react-mailto": "^0.4.0",
    "react-materialize": "^2.4.5",
    "react-media": "^1.8.0",
    "react-meta-tags": "^0.7.1",
    "react-parallax": "^1.8.0",
    "react-protected-mailto": "^1.0.3",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.8",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "react-select": "^2.0.0",
    "react-speech-recognition": "^1.0.7",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.3",
    "redux-form": "^7.4.2",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-thunk": "^2.3.0",
    "sendemail": "^3.4.0",
